   "source": [
    "h = np.linspace(.1,.9,100)\n",
    "y = .4\n",
    "\n",
    "ced = -y*1/h+(1-y)*(1-h)**-1\n",
    "ce = -y*np.log(h)-(1-y)*np.log(1-h)\n",
    "f, ax = plt.subplots(2)\n",
    "ax[0].plot(h, ced)\n",
    "ax[1].plot(h, ce)\n",
    "ax[0].grid()\n",
    "ax[1].grid()\n",
    "plt.show()"
   ]
